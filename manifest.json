
{
  "manifest_version": 3,
  "name": "SpoilFree",
  "version": "1.0",
  "description": "Blurs potential spoiler content on web pages using an AI-powered detection API.",
  "host_permissions": ["<all_urls>"],
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
  "16": "icon16.png",
  "48": "icon48.png",
  "128": "icon128.png"
}

  
  
}
